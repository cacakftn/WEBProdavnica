{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "JwtSettings": {
        "Secret": "SuperSecretKeyForJwtTokenGenerationShouldBeLongEnough", // Tajni kljuƒç za potpisivanje
        "Issuer": "WebShopAuthServer", // Ko je izdao token
        "Audience": "WebShopClient", // Kome je namenjen
        "AccessTokenExpirationMinutes": 60, // Trajanje access tokena
        "RefreshTokenExpirationDays": 7 // Trajanje refresh tokena
    }

}
